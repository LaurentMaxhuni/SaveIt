{
  "manifest_version": 3,
  "name": "SaveIt: Simple File Downloader",
  "version": "1.0",
  "description": "SaveIt is a simple chrome extension where you can input a URL and get the file from that endpoint.",
  "permissions": ["downloads"],
  "action": {
    "default_popup": "popup/popup.html",
    "default_icon": {
      "16": "images/icon-16.png",
      "32": "images/icon-32.png",
      "48": "images/icon-48.png",
      "128": "images/icon-128.png"
    }
  },
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  }
}
